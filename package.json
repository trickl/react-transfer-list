    "prettier": "prettier --single-quote --write 'src/**/*.{jsx,js,ts,tsx,scss}'",
    "storybook": "storybook dev -p 6006 -s public",
    "build-storybook": "storybook build -s public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
